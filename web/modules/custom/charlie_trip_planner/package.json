{
  "name": "charlie-trip-planner",
  "description": "Trip Planner widget using Google Directions/Maps API",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch": "npm-watch",
    "dev": "npm run build && npm run watch",
    "build": "webpack --config webpack.config.js",
    "build:test": "webpack --config webpack.config.js --display-error-details"
  },
  "author": "Mark Labrecque",
  "engines": {
    "node": ">=4.4.7",
    "npm": ">=3.0.0"
  },
  "watch": {
    "build": {
      "patterns": [
        "app"
      ],
      "extensions": "js,jsx",
      "ignore": "node_modules",
      "quiet": true
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.1.0",
    "lib": "^2.2.5",
    "react": "^15.4.2",
    "react-axios": "0.0.9",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "0.23.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "npm-watch": "^0.1.8",
    "scroll-to-element": "^2.0.0",
    "style-loader": "0.13.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-validator": "^3.0.0"
  }
}
